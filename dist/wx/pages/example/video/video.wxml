<c-page title="video" class=" cml-base cml-c-page">
  <view class="container  cml-base cml-view">
    <c-header title="video" class=" cml-base cml-c-header"></c-header>
      <text class=" cml-base cml-text">视频state: </text>
      <text class="info  cml-base cml-text">{{state}}</text>
      <c-video class="video  cml-base cml-c-video" src="{{videoSrc}}" autoplay="{{true}}" controls="{{true}}" bindstart="_cmlEventProxy" data-eventstart="onstart" bindpause="_cmlEventProxy" data-eventpause="onpause" bindfinish="_cmlEventProxy" data-eventfinish="onfinish" bindfail="_cmlEventProxy" data-eventfail="onfail" style="display:flex;flex-direction:column;align-items:center"></c-video>
  </view>
</c-page>